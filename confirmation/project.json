{
  "name": "confirmation",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "confirmation/src",
  "prefix": "easy",
  "tags": [],
  "projectType": "library",
  "targets": {
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "confirmation/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["confirmation/**/*.ts", "confirmation/**/*.html"]
      }
    }
  }
}
